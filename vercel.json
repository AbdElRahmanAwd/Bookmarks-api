{
  "version": 2,
  "builds": [
    {
      "src": "api/[...path].ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/ping.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/ping",
      "dest": "/api/ping.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/[...path].ts"
    }
  ]
}
